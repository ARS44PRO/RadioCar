{
  "expo": {
    "name": "RadioCar",
    "slug": "RadioCar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "supportedInterfaceOrientations": [
        "portrait",
        "landscapeLeft",
        "landscapeRight",
        "portrait",
        "landscapeLeft",
        "landscapeRight",
        "portrait",
        "landscapeLeft",
        "landscapeRight",
        "portrait",
        "landscapeLeft",
        "landscapeRight"
      ],
      "bundleIdentifier": "com.ars44pro.RadioCar",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "package": "com.ars44pro.RadioCar",
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "initialOrientation": "portrait"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a8c50aca-6aef-4374-b9b0-bc2146f23483"
      }
    }
  }
}
